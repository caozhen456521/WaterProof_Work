<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ededed"
    android:fitsSystemWindows="true"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical"
    tools:context="com.qingzu.waterproof_work.MessageSettingActivity" >
 <com.qingzu.uitest.view.MyTitleView
        android:id="@+id/message_setting_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:center_text="消息设置"
        app:left_height="30dp"
        app:left_src="@drawable/selector_title_back"
        app:left_width="40dp"
         app:right_height="30dp"
        app:right_src="@drawable/info_setting"
        app:right_width="40dp"
        
         />
   

</LinearLayout>
