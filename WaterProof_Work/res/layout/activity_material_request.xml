<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    android:fitsSystemWindows="true"
    android:focusable="true"
    android:focusableInTouchMode="true"
    tools:context="com.qingzu.waterproof_work.MaterialRequestActivity" >

    <com.qingzu.uitest.view.MyTitleView
        android:id="@+id/material_request_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:center_text="材料要求"
        app:left_height="30dp"
        app:left_src="@drawable/selector_title_back"
        app:left_width="40dp"
        app:right_text="确定"
         />

    <ListView
        android:id="@+id/material_request_lv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/material_request_title"
        android:descendantFocusability="blocksDescendants"
        android:divider="@null"
        android:scrollbars="none" />

