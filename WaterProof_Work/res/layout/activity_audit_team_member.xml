<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/page_background"
    android:fitsSystemWindows="true"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical"
    tools:context=".AuditTeamMemberActivity" >

    <com.qingzu.uitest.view.MyTitleView
        android:id="@+id/aatm_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:center_text="审批队员"
        app:left_height="30dp"
        app:left_src="@drawable/selector_title_back"
        app:left_width="40dp"
        app:right_text="一键邀请"
        app:title_background="#383b3d" />
    
     <com.qingzu.custom.control.ZListView
        android:id="@+id/aatm_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:descendantFocusability="blocksDescendants"
        android:divider="@null"
        android:listSelector="@android:color/transparent"
        android:scrollbars="none" />

</LinearLayout>