<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:orientation="vertical"
     android:fitsSystemWindows="true"
    android:focusable="true"
    android:focusableInTouchMode="true"
    tools:context="com.qingzu.waterproof_work.ProjectPushActivity" >

  <com.qingzu.uitest.view.MyTitleView
        android:id="@+id/project_push_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:center_text="工程推送"
        app:left_height="30dp"
        app:left_src="@drawable/selector_title_back"
        app:left_width="40dp"
        />

    <com.qingzu.custom.control.ZListView
        android:id="@+id/lv_project_push"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:descendantFocusability="blocksDescendants"
        android:divider="@null"
        android:scrollbars="none" />
    

</LinearLayout>
